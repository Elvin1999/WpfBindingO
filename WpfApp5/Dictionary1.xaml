<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--<SolidColorBrush x:Key="PrimaryColor" >SpringGreen</SolidColorBrush>-->
    <LinearGradientBrush x:Key="PrimaryColor">
        <GradientStop Color="SpringGreen" Offset="0" ></GradientStop>
        <GradientStop Color="Red" Offset="1" ></GradientStop>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="DarkPrimaryColor">
        <GradientStop Color="Black" Offset="0" ></GradientStop>
        <GradientStop Color="Red" Offset="1" ></GradientStop>
    </LinearGradientBrush>
    <sys:Double x:Key="MainSize">40</sys:Double>
</ResourceDictionary>